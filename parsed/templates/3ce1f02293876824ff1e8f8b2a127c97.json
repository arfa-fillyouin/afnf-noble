{
  "components/custom/swatch-on-category": "<div class=\"get-swatches\">\n\t{{#each product.options}}\n\t\t{{{dynamicComponent 'components/products/options-custom'}}} \n\t{{/each}}\n</div> \n",
  "components/products/options-custom/swatch": "<div class=\"form-field\" data-custom-attribute=\"swatch\">\n    {{#each (limit this.values 6)}}\n\t\t<label class=\"form-option form-option-swatch index_{{@index}} {{#if @index '==' 6}}found{{/if}}\" data-product-attribute-value=\"{{id}}\" data-product-attribute-name=\"{{../id}}\"> \n\t\t\t{{#if image}}\n\t\t\t\t<span class='form-option-variant form-option-variant--pattern' title=\"{{this.label}}\" style=\"background-image: url('{{getImage image \"swatch_option_size\"}}');\"></span>\n\t\t\t{{/if}}\n\t\t\t{{#if data.[2]}}\n\t\t\t\t<span class='form-option-variant form-option-variant--color' title=\"{{this.label}}\" style=\"background-color: #{{data.[0]}}\"></span>\n\t\t\t\t<span class='form-option-variant form-option-variant--color' title=\"{{this.label}}\" style=\"background-color: #{{data.[1]}}\"></span>\n\t\t\t\t<span class='form-option-variant form-option-variant--color' title=\"{{this.label}}\" style=\"background-color: #{{data.[2]}}\"></span>\n\t\t\t{{else}}\n\t\t\t\t{{#if data.[1]}} \n\t\t\t\t\t<span class='form-option-variant form-option-variant--color' title=\"{{this.label}}\" style=\"background-color: #{{data.[0]}}\"></span>\n\t\t\t\t\t<span class='form-option-variant form-option-variant--color' title=\"{{this.label}}\" style=\"background-color: #{{data.[1]}}\"></span>\n\t\t\t\t{{else}}\n\t\t\t\t\t{{#if data.[0]}}\n\t\t\t\t\t\t<span class='form-option-variant form-option-variant--color' title=\"{{this.label}}\" style=\"background-color: #{{data.[0]}}\"></span>\n\t\t\t\t\t{{/if}}\n\t\t\t\t{{/if}}\n\t\t\t{{/if}}\n\t\t</label> \n    {{/each}}\n\t{{#each (limit this.values 7)}}\n        {{#if @index \"==\" 6}}\n            <a class=\"lable-more-card\" href=\"{{product.url}}\">+more</a>\n        {{/if}}\n    {{/each}}\n</div>\n"
}