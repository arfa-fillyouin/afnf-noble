{
  "components/custom/custom-navigation-list-alternate": "{{#if children}}\n    <a class=\"navPages-action navPages-action-depth-max has-subMenu is-root{{#if is_active}} activePage{{/if}}\"\n       href=\"{{url}}\"\n       aria-label=\"{{name}}\"\n    >\n        {{name}}\n    </a>\n    <span class=\"menu-icon\">\n        <svg class=\"add-icon\"><use xlink:href=\"#icon-add\" /></svg>\n        <svg class=\"remove-icon\"><use xlink:href=\"#icon-remove\" /></svg>\n    </span>\n    <div class=\"navPage-subMenu altermenu\">\n        <ul class=\"navPage-list\">\n            {{#each children}}\n                <li class=\"navPage-item\">\n                    {{#if children}}\n                        <a class=\"navPages-action has-SubPages\" href=\"{{url}}\">{{name}}<i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\"><svg><use xlink:href=\"#icon-arrow-right\" /></svg></i></a>\n                        <span class=\"menu-icon\">\n                            <svg class=\"add-icon\"><use xlink:href=\"#icon-add\" /></svg>\n                            <svg class=\"remove-icon\"><use xlink:href=\"#icon-remove\" /></svg>\n                        </span>\n                        <div class=\"navPage-childList\">\n                            <ul class=\"navPage-list\">\n                                {{#each (limit children 5)}}\n                                    <li class=\"navPage-item\">\n                                        <a class=\"navPage-SubPages-action navPages-action\" href=\"{{url}}\">{{name}}</a>\n                                    </li> \n                                {{/each}}\n                                <li class=\"navPage-item viewAllLink\">\n                                    <a class=\"navPage-SubPages-action navPages-action\" href=\"{{url}}\">{{lang 'common.view_all'}}</a>\n                                </li> \n                            </ul>\n                        </div>\n                    {{else}}\n                        <a class=\"navPages-action\" href=\"{{url}}\">{{name}}</a>\n                    {{/if}}\n                </li>\n            {{/each}}\n        </ul>\n    </div>\n{{else}}\n    <a class=\"navPages-action{{#if is_active}} activePage{{/if}}\"\n       href=\"{{url}}\"\n       aria-label=\"{{name}}\"\n    >\n        {{name}}\n    </a>\n{{/if}}\n"
