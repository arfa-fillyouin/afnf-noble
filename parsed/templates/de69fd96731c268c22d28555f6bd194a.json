{
  "components/common/header": "<a href=\"#main-content\" class=\"skip-to-main-link\">{{lang 'header.skip_to_main'}}</a>\n\n<!-- Main Header Start -->\n<header class=\"header {{#if theme_settings.light_bg-header}}light-bg-header{{/if}} {{#if theme_settings.sticky_header}}sticky-header{{/if}} {{#if theme_settings.transparent_header}}transparent-header{{/if}} header-{{theme_settings.logo_alignment_option}} {{#if theme_settings.hide_banner}}hide-top-banner{{/if}}\" role=\"banner\">\n    <!-- Global Top Banner Start -->\n    {{#unless theme_settings.hide_banner}}\n        <div class=\"global-banner top\">\n            <p class=\"global-banner-text{{#unless theme_settings.disable_animation}} animate__animated animate__fadeIn{{/unless}}\">{{limit theme_settings.banner-content 100}}</p>\n        </div>\n    {{/unless}}\n    <!-- Global Top Banner End -->\n\n    <!-- Marketing Top Banner Start -->\n    {{#if banners.top}}\n        <div class=\"banners\" data-banner-location=\"top\">\n            {{#each (limit banners.top_metadata 1)}}\n                <div class=\"banner\" data-event-type=\"promotion\" data-entity-id=\"{{this.id}}\" data-name=\"{{this.banner-name}}\" data-position=\"{{this.location}}\" data-banner-id=\"{{this.banner-name}}\">\n                    <div data-event-type=\"promotion-click\">\n                        {{{this.content}}}\n                    </div>\n                </div>\n            {{/each}}\n        </div>\n    {{/if}}\n    <!-- Marketing Top Banner End -->\n\n    <div class=\"header-wrapper{{#unless theme_settings.disable_animation}} animate__animated animate__fadeIn{{/unless}}\">\n        <div class=\"header-wrapper-inner\">\n            <div class=\"mobileMenu-toggle-open\">\n                <span class=\"mobileMenu-toggle-open-inner\">\n                    <svg><use xlink:href=\"#icon-menu\" /></svg>\n                </span>\n            </div>\n            <div class=\"{{#or (if theme_settings.logo_alignment_option '==' 'center') (if theme_settings.logo_alignment_option '===' 'stacked-social') (if theme_settings.logo_alignment_option '===' 'center-stacked') }}header-left{{/or}}{{#if theme_settings.logo_alignment_option '==' 'left'}}header-middle{{/if}}\">\n                {{#or (if theme_settings.logo_alignment_option '==' 'center') (if theme_settings.logo_alignment_option '==' 'left')}}\n                    <div class=\"navPages-container\" id=\"menu\" data-menu>\n                        {{> components/common/navigation-menu}}\n                    </div>\n                {{/or}}\n                {{#if theme_settings.logo_alignment_option '===' 'stacked-social'}}\n                    {{> components/common/social-links}}\n                {{/if}}\n                {{#if theme_settings.logo_alignment_option '===' 'center-stacked'}}\n                    <nav class=\"navUser\">\n                        <ul class=\"navUser-section navUser-section--alt\">\n                            <li class=\"navUser-item navUser--search\">\n                                <button class=\"navUser-action navUser-action--quickSearch\"\n                                   type=\"button\"\n                                   id=\"quick-search-expand-1\"\n                                   data-search=\"quickSearch\"\n                                   aria-controls=\"quickSearch\"\n                                   aria-label=\"{{lang 'common.search'}}\"\n                                >\n                                    <span class=\"navUser-custom-icon\">\n                                        <svg><use xlink:href=\"#icon-search\"></use></svg>\n                                    </span>\n                                </button>\n                            </li>\n                        </ul>\n                    </nav>\n                {{/if}}\n            </div>\n            \n            <div class=\"{{#or (if theme_settings.logo_alignment_option '==' 'center') (if theme_settings.logo_alignment_option '===' 'stacked-social') (if theme_settings.logo_alignment_option '===' 'center-stacked') }}header-middle{{/or}}{{#if theme_settings.logo_alignment_option '==' 'left'}}header-left{{/if}}\">\n                {{#if page_type '===' 'default'}}\n                    <h1 class=\"header-logo\">\n                        {{> components/common/store-logo}}\n                    </h1>\n                {{else}}\n                    <div class=\"header-logo\">\n                        {{> components/common/store-logo}}\n                    </div>\n                {{/if}}\n            </div>\n            \n            <div class=\"header-right\">\n                {{> components/common/navigation}}\n                {{#if theme_settings.logo_alignment_option '===' 'left'}}\n                    <div class=\"mobileMenu-toggle-open\">\n                        <span class=\"mobileMenu-toggle-open-inner\">\n                            <svg><use xlink:href=\"#icon-menu\" /></svg>\n                        </span>\n                    </div>\n                {{/if}}\n            </div>\n        </div>\n        {{#or (if theme_settings.logo_alignment_option '==' 'center-stacked') (if theme_settings.logo_alignment_option '==' 'stacked-social')}}\n            <div class=\"header-stacked\">\n                <div class=\"navPages-container\" id=\"menu\" data-menu>\n                    {{> components/common/navigation-menu}}\n                </div>\n            </div>\n        {{/or}}\n    </div>\n</header>\n{{!-- <div class=\"dropdown dropdown--quickSearch\" id=\"quickSearch\" aria-hidden=\"true\" data-prevent-quick-search-close>\n    {{!> components/common/quick-search name='nav-quick-search'}}\n</div> --}}\n<!-- Main Header End -->\n\n{{{region name=\"header_bottom--global\"}}}\n{{{region name=\"header_bottom\"}}}\n",
  "components/common/navigation-menu": "<nav class=\"navPages\">\n    <div class=\"mobileMenu-toggle-close\">\n        <span class=\"mobileMenu-toggle-close-inner\">\n            <svg><use xlink:href=\"#icon-close\" /></svg>\n        </span>\n    </div>\n    <ul class=\"navPages-list{{#if theme_settings.navigation_design '!==' 'simple'}} navPages-list-depth-max{{/if}} navPages-list-mobile\">\n        {{#unless theme_settings.hide_content_navigation}}\n            <li class=\"navPages-item navPages-item--shop{{#and (if theme_settings.hide_submenu_img_section) (if categories.length '<' 1)}} navPages-item--hidden{{/and}}{{#if categories.length '<' 1}} navPages-item--hidden--mobile{{/if}}\">\n                <a class=\"navPages-action\" href=\"javascript:void(0)\">\n                    {{lang 'common.shop'}}\n                </a>\n                \n                <span class=\"menu-icon\">\n                    <svg class=\"add-icon\"><use xlink:href=\"#icon-add\" /></svg>\n                    <svg class=\"remove-icon\"><use xlink:href=\"#icon-remove\" /></svg>\n                </span>\n                \n                {{#if theme_settings.navigation_design '===' 'simple'}}\n                    {{> components/custom/custom-navigation-dropdown}}\n                {{/if}}\n                {{#if theme_settings.navigation_design '===' 'alternate'}}\n                    {{> components/custom/custom-navigation-dropdown-alternate}}\n                {{/if}}\n            </li>\n            {{#each pages}}\n                <li class=\"navPages-item nav-pages-Items{{#if @index '===' 0}} navPages-item--content-first{{/if}}\">\n                    {{> components/custom/custom-navigation-pages}} \n                </li>\n             {{/each}}\n        {{else}}\n            {{#or (if theme_settings.logo_alignment_option '==' 'center') (if theme_settings.logo_alignment_option '==' 'left')}}\n                {{#each (limit categories 5)}}\n                    <li class=\"navPages-item\">\n                    {{#if ../theme_settings.navigation_design '===' 'simple'}}\n                        {{> components/custom/custom-navigation-list}}\n                    {{/if}}\n                    {{#if ../theme_settings.navigation_design '===' 'alternate'}}\n                        {{> components/custom/custom-navigation-list-alternate}}\n                    {{/if}}\n                    </li>\n                {{/each}}\n            {{else}}\n                {{#each categories}}\n                    <li class=\"navPages-item\">\n                    {{#if ../theme_settings.navigation_design '===' 'simple'}}\n                        {{> components/custom/custom-navigation-list}}\n                    {{/if}}\n                    {{#if ../theme_settings.navigation_design '===' 'alternate'}}\n                        {{> components/custom/custom-navigation-list-alternate}}\n                    {{/if}}\n                    </li>\n                {{/each}}\n            {{/or}}\n        {{/unless}}\n    </ul>\n    \n    <ul class=\"navPages-list navPages-list--user\">\n        {{#if currency_selector.currencies.length '>' 1}}\n            <li class=\"navPages-item\">\n                <a class=\"navPages-action has-subMenu\"\n                   href=\"#\"\n                   data-collapsible=\"navPages-currency\"\n                   aria-controls=\"navPages-currency\"\n                   aria-expanded=\"false\"\n                   aria-label=\"{{lang 'common.currency' code=currency_selector.active_currency_code}}\"\n                >\n                    {{lang 'common.currency' code=currency_selector.active_currency_code}}\n                </a>\n                <span class=\"menu-icon\">\n                    <svg class=\"add-icon\"><use xlink:href=\"#icon-add\" /></svg>\n                    <svg class=\"remove-icon\"><use xlink:href=\"#icon-remove\" /></svg>\n                </span>\n                <div class=\"navPage-subMenu\" id=\"navPages-currency\" aria-hidden=\"true\" tabindex=\"-1\">\n                    <ul class=\"navPage-subMenu-list\">\n                        {{#each currency_selector.currencies}}\n                        <li class=\"navPage-subMenu-item\">\n                            <a class=\"navPage-subMenu-action navPages-action\"\n                               href=\"{{{switch_url}}}\"\n                               aria-label=\"{{name}}\"\n                               data-currency-code=\"{{{code}}}\"\n                               {{#if cart_currency_switch_url.length '>' 0 }}\n                               data-cart-currency-switch-url=\"{{{cart_currency_switch_url}}}\"\n                               {{/if}}\n                               data-warning=\"{{lang 'common.currency_switch_promotion'}}\"\n                            >\n                                {{#if is_active}}\n                                    <strong>{{name}}</strong>\n                                {{else}}\n                                    {{name}}\n                                {{/if}}\n                            </a>\n                        </li>\n                        {{/each}}\n                    </ul>\n                </div>\n            </li>\n        {{/if}}\n        {{#if settings.gift_certificates_enabled}}\n            <li class=\"navPages-item\">\n                <a class=\"navPages-action\"\n                   href=\"{{urls.gift_certificate.purchase}}\"\n                   aria-label=\"{{lang 'common.gift_cert'}}\"\n                >\n                    {{lang 'common.gift_cert'}}\n                </a>\n            </li>\n        {{/if}}\n        {{#if settings.show_wishlist}}\n            <li class=\"navPages-item navPages--wishlist\">\n                <a class=\"navPages-action\"\n                   href=\"{{urls.account.wishlists.all}}\"\n                   aria-label=\"wishlist\"\n                >\n                    {{lang 'account.mobile_nav.wishlists'}}\n                </a>\n            </li>\n        {{/if}}\n        {{#if customer.store_credit.value '>' 0}}\n            <li class=\"navPages-item\">\n                <a class=\"navPages-action navPages-action--storeCredit\">\n                    {{lang 'common.store_credit_overview' credit=customer.store_credit.formatted}}\n                </a>\n            </li>\n        {{/if}}\n        {{#if theme_settings.product_list_display_mode '==' 'list'}}\n            <li class=\"navPages-item\">\n                <a class=\"navPages-action navPages-action--compare\"\n                   href=\"{{urls.compare}}\"\n                   data-compare-nav\n                   aria-label=\"{{lang 'common.compare'}}\"\n                >\n                    {{lang 'common.compare'}}\n                    <span class=\"countPill countPill--positive countPill--alt\"></span>\n                </a>\n            </li>\n        {{/if}}\n        {{#if customer}}\n            <li class=\"navPages-item\">\n                <a class=\"navPages-action has-subMenu\"\n                   href=\"{{urls.account.index}}\"\n                   data-collapsible=\"navPages-account\"\n                   aria-label=\"{{lang 'common.account'}}\"\n                >\n                    {{lang 'common.account'}}\n                </a>\n                <span class=\"menu-icon\">\n                    <svg class=\"add-icon\"><use xlink:href=\"#icon-add\" /></svg>\n                    <svg class=\"remove-icon\"><use xlink:href=\"#icon-remove\" /></svg>\n                </span>\n                <div class=\"navPage-subMenu\" id=\"navPages-account\" aria-hidden=\"true\" tabindex=\"-1\">\n                    <ul class=\"navPage-subMenu-list\">\n                        <li class=\"navPage-subMenu-item\">\n                            <a class=\"navPage-subMenu-action navPages-action\"\n                               href=\"{{urls.account.orders.all}}\"\n                               aria-label=\"{{lang 'account.nav.orders'}}\"\n                            >\n                                {{lang 'account.nav.orders'}}\n                            </a>\n                        </li>\n                        {{#if settings.returns_enabled}}\n                            <li class=\"navPage-subMenu-item\">\n                                <a class=\"navPage-subMenu-action navPages-action\"\n                                   href=\"{{urls.account.returns}}\"\n                                   aria-label=\"{{lang 'account.nav.returns'}}\"\n                                >\n                                    {{lang 'account.nav.returns'}}\n                                </a>\n                            </li>\n                        {{/if}}\n                        <li class=\"navPage-subMenu-item\">\n                            <a class=\"navPage-subMenu-action navPages-action\"\n                               href=\"{{urls.account.inbox}}\"\n                               aria-label=\"{{lang 'account.mobile_nav.messages'}}\"\n                            >\n                                {{lang 'account.mobile_nav.messages'}}\n                            </a>\n                        </li>\n                        <li class=\"navPage-subMenu-item\">\n                            <a class=\"navPage-subMenu-action navPages-action\"\n                               href=\"{{urls.account.addresses}}\"\n                               aria-label=\"{{lang 'account.nav.addresses'}}\"\n                            >\n                                {{lang 'account.nav.addresses'}}\n                            </a>\n                        </li>\n                        {{#if settings.show_payment_methods}}\n                            <li class=\"navPage-subMenu-item\">\n                                <a class=\"navPage-subMenu-action navPages-action\"\n                                   href=\"{{urls.account.payment_methods.all}}\"\n                                   aria-label=\"{{lang 'account.nav.payment_methods'}}\"\n                                >\n                                    {{lang 'account.nav.payment_methods'}}\n                                </a>\n                            </li>\n                        {{/if}}\n                        <li class=\"navPage-subMenu-item\">\n                            <a class=\"navPage-subMenu-action navPages-action\"\n                               href=\"{{urls.account.wishlists.all}}\"\n                               aria-label=\"{{lang 'account.mobile_nav.wishlists'}}\"\n                            >\n                                {{lang 'account.mobile_nav.wishlists'}}\n                            </a>\n                        </li>\n                        <li class=\"navPage-subMenu-item\">\n                            <a class=\"navPage-subMenu-action navPages-action\"\n                               href=\"{{urls.account.recent_items}}\"\n                               aria-label=\"{{lang 'account.nav.recently_viewed'}}\"\n                            >\n                                {{lang 'account.nav.recently_viewed'}}\n                            </a>\n                        </li>\n                        <li class=\"navPage-subMenu-item\">\n                            <a class=\"navPage-subMenu-action navPages-action\"\n                               href=\"{{urls.account.details}}\"\n                               aria-label=\"{{lang 'account.nav.settings'}}\"\n                            >\n                                {{lang 'account.nav.settings'}}\n                            </a>\n                        </li>\n                        <li class=\"navPage-subMenu-item\">\n                            <a class=\"navPage-subMenu-action navPages-action\"\n                               href=\"{{urls.auth.logout}}\"\n                               aria-label=\"{{lang 'common.logout'}}\"\n                            >\n                                {{lang 'common.logout'}}\n                            </a>\n                        </li>\n                    </ul>\n                </div>\n            </li>\n        {{else}}\n            <li class=\"navPages-item\">\n                <a class=\"navPages-action\"\n                   href=\"{{urls.auth.login}}\"\n                   aria-label=\"{{lang 'common.login'}}\"\n                >\n                    {{lang 'common.login'}}\n                </a>\n                {{#if settings.account_creation_enabled}}\n                    <span class=\"or-label\">{{lang 'common.or'}}</span>\n                    <a class=\"navPages-action\"\n                       href=\"{{urls.auth.create_account}}\"\n                       aria-label=\"{{lang 'common.sign_up'}}\"\n                    >\n                        {{lang 'common.sign_up'}}\n                    </a>\n                {{/if}}\n            </li>\n        {{/if}}\n    </ul>\n\n    <div class=\"mobile-social-icons\">\n        {{> components/common/social-links}}\n    </div>\n \n</nav>\n",
  "components/common/social-links": "{{#if social_media}}\n    <ul class=\"socialLinks socialLinks--alt\">\n        {{#each social_media}}\n            <li class=\"socialLinks-item\">\n                <a class=\"icon icon--{{display_name}}\" href=\"{{url}}\" target=\"_blank\" rel=\"noopener\" aria-label=\"icon of {{display_name}}\">\n                    <svg><use xlink:href=\"#icon-{{display_name}}\" /></svg>\n                </a>\n            </li>\n        {{/each}}\n    </ul>\n{{/if}}\n",
  "components/common/navigation": "<nav class=\"navUser\">\n    {{#or customer (unless theme_settings.restrict_to_login)}}\n        {{> components/common/currency-selector}}\n    {{/or}}\n\n    <ul class=\"navUser-section navUser-section--alt\">\n        {{#if customer.store_credit.value '>' 0}}\n            <li class=\"navUser-item\">\n                <a class=\"navUser-action navUser-action--storeCredit\"\n                   data-dropdown=\"storeCredit-dropdown\"\n                   data-options=\"align:bottom\"\n                   href=\"{{urls.cart}}\"\n                   aria-label=\"{{lang 'common.store_credit_overview' credit=customer.store_credit.formatted}}\"\n                >\n                    <span class=\"navUser-action-divider\">\n                        {{lang 'common.store_credit_overview' credit=customer.store_credit.formatted}}\n                    </span>\n                </a>\n                <div class=\"dropdown-menu\" id=\"storeCredit-dropdown\" data-dropdown-content aria-hidden=\"true\">\n                    {{{lang 'common.store_credit' store_credit=customer.store_credit.formatted}}}\n                </div>\n            </li>\n        {{/if}}\n        \n        <li class=\"navUser-item navUser--compare\">\n            <a class=\"navUser-action navUser-item--compare\"\n               href=\"{{urls.compare}}\"\n               data-compare-nav\n               aria-label=\"{{lang 'common.compare'}}\"\n            >\n                <span class=\"navUser-custom-icon\">\n                    <svg><use xlink:href=\"#icon-compare\"></use></svg>\n                </span>\n                <span class=\"countPill countPill--positive countPill--alt\">0</span>\n            </a>\n        </li>\n        {{#if settings.gift_certificates_enabled}}\n            <li class=\"navUser-item navUser--gift\">\n                <a class=\"navUser-action\"\n                   href=\"{{urls.gift_certificate.purchase}}\"\n                   aria-label=\"{{lang 'common.gift_cert'}}\"\n                >\n                    <span class=\"navUser-custom-icon\">\n                        <svg><use xlink:href=\"#icon-giftcard\"></use></svg>\n                    </span>\n                </a>\n            </li>\n        {{/if}}\n        {{#if settings.show_wishlist}}\n            <li class=\"navUser-item navUser--wishlist\">\n                <a class=\"navUser-action\"\n                   href=\"{{urls.account.wishlists.all}}\"\n                   aria-label=\"wishlist\"\n                >\n                    <span class=\"navUser-custom-icon\">\n                        <svg><use xlink:href=\"#icon-heart\"></use></svg>\n                    </span>\n                </a>\n            </li>\n        {{/if}}\n        <li class=\"navUser-item navUser-item--account\">\n            <a class=\"navUser-action\"\n               href=\"{{urls.auth.login}}\"\n               aria-label=\"{{lang 'common.login'}}\"\n            >\n                <span class=\"navUser-custom-icon\">\n                    <svg><use xlink:href=\"#icon-account\"></use></svg>\n                </span>\n            </a>\n            {{#if customer}}\n            <div class=\"navUser-dropdown dropdown-menu\">\n                <ul class=\"navUser-list\">\n                    <li class=\"navUser-item\">\n                        <a class=\"navUser-action\"\n                           href=\"{{urls.account.orders.all}}\"\n                           aria-label=\"{{lang 'account.nav.orders'}}\"\n                        >\n                            {{lang 'account.nav.orders'}}\n                        </a>\n                    </li>\n                    {{#if settings.returns_enabled}}\n                        <li class=\"navUser-item\">\n                            <a class=\"navUser-action\"\n                               href=\"{{urls.account.returns}}\"\n                               aria-label=\"{{lang 'account.nav.returns'}}\"\n                            >\n                                {{lang 'account.nav.returns'}}\n                            </a>\n                        </li>\n                    {{/if}}\n                    <li class=\"navUser-item\">\n                        <a class=\"navUser-action\"\n                           href=\"{{urls.account.inbox}}\"\n                           aria-label=\"{{lang 'account.mobile_nav.messages'}}\"\n                        >\n                            {{lang 'account.mobile_nav.messages'}}\n                        </a>\n                    </li>\n                    <li class=\"navUser-item\">\n                        <a class=\"navUser-action\"\n                           href=\"{{urls.account.addresses}}\"\n                           aria-label=\"{{lang 'account.nav.addresses'}}\"\n                        >\n                            {{lang 'account.nav.addresses'}}\n                        </a>\n                    </li>\n                    {{#if settings.show_payment_methods}}\n                        <li class=\"navUser-item\">\n                            <a class=\"navUser-action\"\n                               href=\"{{urls.account.payment_methods.all}}\"\n                               aria-label=\"{{lang 'account.nav.payment_methods'}}\"\n                            >\n                                {{lang 'account.nav.payment_methods'}}\n                            </a>\n                        </li>\n                    {{/if}}\n                    <li class=\"navUser-item\">\n                        <a class=\"navUser-action\"\n                           href=\"{{urls.account.wishlists.all}}\"\n                           aria-label=\"{{lang 'account.mobile_nav.wishlists'}}\"\n                        >\n                            {{lang 'account.mobile_nav.wishlists'}}\n                        </a>\n                    </li>\n                    <li class=\"navUser-item\">\n                        <a class=\"navUser-action\"\n                           href=\"{{urls.account.recent_items}}\"\n                           aria-label=\"{{lang 'account.nav.recently_viewed'}}\"\n                        >\n                            {{lang 'account.nav.recently_viewed'}}\n                        </a>\n                    </li>\n                    <li class=\"navUser-item\">\n                        <a class=\"navUser-action\"\n                           href=\"{{urls.account.details}}\"\n                           aria-label=\"{{lang 'account.nav.settings'}}\"\n                        >\n                            {{lang 'account.nav.settings'}}\n                        </a>\n                    </li>\n                    <li class=\"navUser-item\">\n                        <a class=\"navUser-action\"\n                           href=\"{{urls.auth.logout}}\"\n                           aria-label=\"{{lang 'common.logout'}}\"\n                        >\n                            {{lang 'common.logout'}}\n                        </a>\n                    </li>\n                </ul>\n            </div>\n            {{/if}}\n        </li>\n        <li class=\"navUser-item navUser--search\">\n            <button class=\"navUser-action navUser-action--quickSearch\"\n               type=\"button\"\n               id=\"quick-search-expand\"\n               data-search=\"quickSearch\"\n               aria-controls=\"quickSearch\"\n               aria-label=\"{{lang 'common.search'}}\"\n            >\n                <span class=\"navUser-custom-icon\">\n                    <svg><use xlink:href=\"#icon-search\"></use></svg>\n                </span>\n            </button>\n        </li>\n        <li class=\"navUser-item navUser-item--cart\">\n            <a class=\"navUser-action\"\n               data-cart-preview\n               data-dropdown=\"cart-preview-dropdown\"\n               data-options=\"align:right\"\n               href=\"{{urls.cart}}\"\n               aria-label=\"{{lang 'cart.nav_aria_label'}}\"\n            >\n                <span class=\"navUser-item-cartLabel navUser-custom-icon\">\n                    <svg><use xlink:href=\"#icon-cart\"></use></svg>\n                </span>\n                <span class=\"countPill cart-quantity\"></span>\n            </a>\n\n            <div class=\"dropdown-menu\" id=\"cart-preview-dropdown\" data-dropdown-content aria-hidden=\"true\"></div>\n        </li>\n    </ul>\n    <div class=\"dropdown dropdown--quickSearch\" id=\"quickSearch\" aria-hidden=\"true\" data-prevent-quick-search-close>\n        {{> components/common/quick-search name='nav-quick-search'}}\n    </div>\n</nav>\n",
  "components/common/store-logo": "<a href=\"{{urls.home}}\" class=\"header-logo__link  {{#if theme_settings.transparent-header-logo-file-title}}transparent-header-logo{{/if}}\" data-header-logo-link>\n    {{#if settings.store_logo.image}}\n        {{#if theme_settings.logo_size '===' 'original'}}\n        <img class=\"header-logo-image-unknown-size\"\n             src=\"{{getImage settings.store_logo.image 'logo_size'}}\"\n             alt=\"{{settings.store_logo.title}}\"\n             title=\"{{settings.store_logo.title}}\">\n        {{else}}\n        <div class=\"header-logo-image-container\">\n            <img class=\"header-logo-image\"\n                 src=\"{{getImage settings.store_logo.image 'logo_size'}}\"\n                 srcset=\"{{getImageSrcset1x2x settings.store_logo.image theme_settings.logo_size}}\"\n                 alt=\"{{settings.store_logo.title}}\"\n                 title=\"{{settings.store_logo.title}}\">\n        </div>\n        {{/if}}\n    {{else if theme_settings.svg_logo}}\n        <img class=\"header-logo-image\"\n             src=\"/content/logo/{{theme_settings.svg-file-title}}\"\n             alt=\"{{settings.store_logo.title}}\"\n             title=\"{{settings.store_logo.title}}\"\n             width=\"{{#if theme_settings.logo-width '===' ''}}200{{else}}{{theme_settings.logo-width}}{{/if}}\" \n             height=\"{{#if theme_settings.logo-height '===' ''}}50{{else}}{{theme_settings.logo-height}}{{/if}}\">\n    {{else}}\n    <span class=\"header-logo-text\">{{settings.store_logo.title}}</span>\n    {{/if}}\n    {{#if theme_settings.transparent-header-logo-file-title}}\n        <img class=\"header-logo-image transparent-header-logo-image\"\n             src=\"/content/logo/{{theme_settings.transparent-header-logo-file-title}}\"\n             alt=\"{{settings.store_logo.title}}\"\n             title=\"{{settings.store_logo.title}}\"\n             width=\"{{#if theme_settings.logo-width '===' ''}}200{{else}}{{theme_settings.logo-width}}{{/if}}\" \n             height=\"{{#if theme_settings.logo-height '===' ''}}50{{else}}{{theme_settings.logo-height}}{{/if}}\">\n    {{/if}}    \n</a>\n",
  "components/custom/custom-navigation-dropdown": "<div class=\"navPage-subMenu{{#if categories.length '<' 1}} no-categories{{/if}}{{#and (if theme_settings.hide_submenu_img_section) (if categories.length '<' 1)}} no-categories-and-image{{/and}}\" aria-hidden=\"true\" tabindex=\"-1\">\n    <div class=\"navPage-subMenu-inner {{#unless theme_settings.hide_submenu_img_section}}{{else}}no-submenu-img{{/unless}}\">\n        <ul class=\"navPage-subMenu-list\">\n            {{#each categories}} \n                <li class=\"navPage-subMenu-item\">\n                    {{#if children}}\n                        <a class=\"navPages-action has-subMenu\" href=\"{{url}}\">\n                            {{name}}\n                        </a>\n                        <span class=\"menu-icon\">\n                            <svg class=\"add-icon\"><use xlink:href=\"#icon-add\" /></svg>\n                            <svg class=\"remove-icon\"><use xlink:href=\"#icon-remove\" /></svg>\n                        </span>\n                        <ul class=\"navPage-childList\">\n                            {{#each (limit children 5)}}\n                            <li class=\"navPage-childList-item\">\n                                <a class=\"navPage-childList-action navPages-action\" href=\"{{url}}\" aria-label=\"{{name}}\">{{name}}</a>\n                            </li>\n                            {{/each}}\n                            <li class=\"navPage-childList-item viewAllLink\">\n                                <a class=\"navPage-childList-action navPages-action\" href=\"{{url}}\">{{lang 'common.view_all'}}</a>\n                            </li>\n                        </ul>\n                    {{else}}\n                        <a class=\"navPages-action has-subMenu\" href=\"{{url}}\">{{name}}</a>\n                    {{/if}}\n                </li>\n            {{/each}}\n        </ul>\n        {{#unless theme_settings.hide_submenu_img_section}}\n        <div class=\"navPage-subMenu-image\">\n            <div class=\"navPage-subMenu-image-content\">\n                {{#if theme_settings.submenu-img}}\n                <a href=\"{{theme_settings.submenu-link}}\">\n                    <img src=\"/product_images/uploaded_images/{{theme_settings.submenu-img}}\">\n                </a>\n                {{else}}\n                    <img src=\"/assets/img/ProductDefault.gif\" class=\"alternate-img\">\n                {{/if}}\n                {{#if theme_settings.submenu-label}}\n                    <span class=\"navPage-subMenu-image-label\">{{theme_settings.submenu-label}}</span>\n                {{/if}}\n                {{#if theme_settings.submenu-sub-label}}\n                    <a href=\"{{theme_settings.submenu-link}}\" class=\"navPage-subMenu-image-sublabel\">{{theme_settings.submenu-sub-label}}</a>\n                {{/if}}\n            </div>\n        </div>\n        {{/unless}}\n    </div>\n</div>\n",
  "components/custom/custom-navigation-dropdown-alternate": "<div class=\"navPage-subMenu altermenu\" aria-hidden=\"true\" tabindex=\"-1\">\n    <ul class=\"navPage-list\">\n        {{#each categories}} \n            <li class=\"navPage-item\">\n                {{#if children}}\n                    <a class=\"navPages-action has-SubPages\" href=\"{{url}}\">{{name}}<i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\"><svg><use xlink:href=\"#icon-arrow-right\" /></svg></i></a>\n                    <span class=\"menu-icon\">\n                        <svg class=\"add-icon\"><use xlink:href=\"#icon-add\" /></svg>\n                        <svg class=\"remove-icon\"><use xlink:href=\"#icon-remove\" /></svg>\n                    </span>\n                    <div class=\"navPage-childList\">\n                        <ul class=\"navPage-list\">\n                            {{#each (limit children 5)}}\n                                <li class=\"navPage-item\">\n                                    <a class=\"navPage-SubPages-action navPages-action\" href=\"{{url}}\">{{name}}</a>\n                                </li> \n                            {{/each}}\n                            <li class=\"navPage-item viewAllLink\">\n                                <a class=\"navPage-SubPages-action navPages-action\" href=\"{{url}}\">{{lang 'common.view_all'}}</a>\n                            </li> \n                        </ul>\n                    </div>\n                {{else}}\n                    <a class=\"navPages-action\" href=\"{{url}}\">{{name}}</a>\n                {{/if}}\n\n            </li> \n        {{/each}}\n    </ul>\n</div>\n",
  "components/custom/custom-navigation-pages": "{{#if children}}\n    <a class=\"navPages-action has-SubPages\" href=\"{{url}}\">\n    \t{{name}}\n    </a> \n    <span class=\"menu-icon\">\n        <svg class=\"add-icon\"><use xlink:href=\"#icon-add\" /></svg>\n        <svg class=\"remove-icon\"><use xlink:href=\"#icon-remove\" /></svg>\n    </span>\n    <div class=\"navPage-subMenu altermenu\">\n\t\t<ul class=\"navPage-list\">\n\t\t\t{{#each children}}\n\t\t\t\t<li class=\"navPage-item\">\n\t\t\t\t\t{{#if children}}\n\t\t\t\t\t\t<a class=\"navPages-action has-SubPages\" href=\"{{url}}\">{{name}}<i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\"><svg><use xlink:href=\"#icon-arrow-right\" /></svg></i></a>\n\t\t\t\t\t\t<span class=\"menu-icon\">\n\t\t\t\t\t        <svg class=\"add-icon\"><use xlink:href=\"#icon-add\" /></svg>\n\t\t\t\t\t        <svg class=\"remove-icon\"><use xlink:href=\"#icon-remove\" /></svg>\n\t\t\t\t\t    </span>\n\t\t\t\t\t\t<div class=\"navPage-childList\">\n\t\t\t\t\t\t\t<ul class=\"navPage-list\">\n\t\t\t\t\t\t\t\t{{#each children}}\n\t\t\t\t\t\t\t\t\t<li class=\"navPage-item\">\n\t\t\t\t\t\t\t\t\t\t<a class=\"navPage-SubPages-action navPages-action\" href=\"{{url}}\">{{name}}</a>\n\t\t\t\t\t\t\t\t\t</li> \n\t\t\t\t\t\t\t\t{{/each}}\n\t\t\t\t\t\t\t</ul>\n\t\t\t\t\t\t</div>\n\t\t\t\t\t{{else}}\n\t\t\t\t\t\t<a class=\"navPages-action\" href=\"{{url}}\">{{name}}</a>\n\t\t\t\t\t{{/if}}\n\n\t\t\t\t</li> \n\t\t\t{{/each}}\n\t\t</ul>\n\t</div>\n{{else}}\n    <a class=\"navPages-action\" href=\"{{url}}\">{{name}}</a>\n{{/if}}\n",
  "components/custom/custom-navigation-list": "{{#if children}}\n<a class=\"navPages-action has-subMenu{{#if is_active}} activePage{{/if}}\"\n   href=\"{{url}}\">\n    {{name}}\n</a>\n<span class=\"menu-icon\">\n    <svg class=\"add-icon\"><use xlink:href=\"#icon-add\" /></svg>\n    <svg class=\"remove-icon\"><use xlink:href=\"#icon-remove\" /></svg>\n</span>\n<div class=\"navPage-subMenu\">\n    <div class=\"navPage-subMenu-inner no-submenu-img\">\n        <ul class=\"navPage-subMenu-list\">\n            {{#each children}}\n                <li class=\"navPage-subMenu-item\">\n                    {{#if children}}\n                        <a class=\"navPages-action has-subMenu{{#if is_active}} activePage{{/if}}\"\n                           href=\"{{url}}\"\n                           aria-label=\"{{name}}\"\n                        >\n                            {{name}}\n                        </a>\n                        <span class=\"menu-icon\">\n                            <svg class=\"add-icon\"><use xlink:href=\"#icon-add\" /></svg>\n                            <svg class=\"remove-icon\"><use xlink:href=\"#icon-remove\" /></svg>\n                        </span>\n                        <ul class=\"navPage-childList\" id=\"navPages-{{id}}\">\n                            {{#each children}}\n                            <li class=\"navPage-childList-item\">\n                                <a class=\"navPage-childList-action navPages-action{{#if is_active}} activePage{{/if}}\"\n                                   href=\"{{url}}\"\n                                   aria-label=\"{{name}}\"\n                                >\n                                    {{name}}\n                                </a>\n                            </li>\n                            {{/each}}\n                        </ul>\n                    {{else}}\n                        <a class=\"navPage-subMenu-action navPages-action{{#if is_active}} activePage{{/if}}\"\n                           href=\"{{url}}\"\n                           aria-label=\"{{name}}\"\n                        >\n                            {{name}}\n                        </a>\n                    {{/if}}\n                </li>\n            {{/each}}\n        </ul>\n    </div>\n</div>\n{{else}}\n<a class=\"navPages-action{{#if is_active}} activePage{{/if}}\"\n   href=\"{{url}}\"\n   aria-label=\"{{name}}\"\n>\n    {{name}}\n</a>\n{{/if}}\n",
  "components/custom/custom-navigation-list-alternate": "{{#if children}}\n    <a class=\"navPages-action navPages-action-depth-max has-subMenu is-root{{#if is_active}} activePage{{/if}}\"\n       href=\"{{url}}\"\n       aria-label=\"{{name}}\"\n    >\n        {{name}}\n    </a>\n    <span class=\"menu-icon\">\n        <svg class=\"add-icon\"><use xlink:href=\"#icon-add\" /></svg>\n        <svg class=\"remove-icon\"><use xlink:href=\"#icon-remove\" /></svg>\n    </span>\n    <div class=\"navPage-subMenu altermenu\">\n        <ul class=\"navPage-list\">\n            {{#each children}}\n                <li class=\"navPage-item\">\n                    {{#if children}}\n                        <a class=\"navPages-action has-SubPages\" href=\"{{url}}\">{{name}}<i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\"><svg><use xlink:href=\"#icon-arrow-right\" /></svg></i></a>\n                        <span class=\"menu-icon\">\n                            <svg class=\"add-icon\"><use xlink:href=\"#icon-add\" /></svg>\n                            <svg class=\"remove-icon\"><use xlink:href=\"#icon-remove\" /></svg>\n                        </span>\n                        <div class=\"navPage-childList\">\n                            <ul class=\"navPage-list\">\n                                {{#each (limit children 5)}}\n                                    <li class=\"navPage-item\">\n                                        <a class=\"navPage-SubPages-action navPages-action\" href=\"{{url}}\">{{name}}</a>\n                                    </li> \n                                {{/each}}\n                                <li class=\"navPage-item viewAllLink\">\n                                    <a class=\"navPage-SubPages-action navPages-action\" href=\"{{url}}\">{{lang 'common.view_all'}}</a>\n                                </li> \n                            </ul>\n                        </div>\n                    {{else}}\n                        <a class=\"navPages-action\" href=\"{{url}}\">{{name}}</a>\n                    {{/if}}\n                </li>\n            {{/each}}\n        </ul>\n    </div>\n{{else}}\n    <a class=\"navPages-action{{#if is_active}} activePage{{/if}}\"\n       href=\"{{url}}\"\n       aria-label=\"{{name}}\"\n    >\n        {{name}}\n    </a>\n{{/if}}\n",
  "components/common/quick-search": "<form class=\"form\" onsubmit=\"return false\" data-url=\"{{urls.search}}\" data-quick-search-form>\n    <fieldset class=\"form-fieldset\">\n        <div class=\"form-field\">\n            <label class=\"is-srOnly\" for=\"{{name}}\">{{lang \"search.quick_search.input_label\"}}</label>\n            <span class=\"navUser-custom-icon\">\n                <svg><use xlink:href=\"#icon-search\"></use></svg>\n            </span>\n            <input class=\"form-input\"\n               data-search-quick\n               name=\"{{name}}\"\n               id=\"{{name}}\"\n               data-error-message=\"{{lang 'search.error.empty_field'}}\"\n               placeholder=\"{{lang 'search.quick_search.input_placeholder'}}\"\n               autocomplete=\"off\"\n            >\n        </div>\n    </fieldset>\n</form>\n<section class=\"quickSearchResults\" data-bind=\"html: results\"></section>\n<p role=\"status\"\n   aria-live=\"polite\"\n   class=\"aria-description--hidden\"\n   data-search-aria-message-predefined-text=\"{{{lang 'search.results.quick_count_live'}}}\"\n></p>\n",
  "components/common/currency-selector": "{{#if currency_selector.currencies.length '>' 1}}\n<ul class=\"navUser-section\">\n    <li class=\"navUser-item navUser--currencySelector\">\n        <a class=\"navUser-action navUser-action--currencySelector has-dropdown\"\n           href=\"#\"\n           data-dropdown=\"currencySelection\"\n           aria-controls=\"currencySelection\"\n           aria-expanded=\"false\"\n        >\n            {{lang 'common.currency' code=currency_selector.active_currency_code}}\n            <i class=\"icon\" aria-hidden=\"true\">\n                <svg>\n                    <use xlink:href=\"#icon-chevron-down\" />\n                </svg>\n            </i>\n        </a>\n        <ul class=\"dropdown-menu\" id=\"currencySelection\" data-dropdown-content aria-hidden=\"true\" tabindex=\"-1\">\n            {{#each currency_selector.currencies}}\n            <li class=\"dropdown-menu-item\">\n                <a href=\"{{{switch_url}}}\"\n                   data-currency-code=\"{{{code}}}\"\n                   {{#if cart_currency_switch_url.length '>' 0 }}\n                   data-cart-currency-switch-url=\"{{{cart_currency_switch_url}}}\"\n                   {{/if}}\n                   data-warning=\"{{lang 'common.currency_switch_promotion'}}\"\n                >\n                {{#if is_active}}\n                    <strong>{{name}}</strong>\n                {{else}}\n                    {{name}}\n                {{/if}}\n                </a>\n            </li>\n            {{/each}}\n        </ul>\n    </li>\n</ul>\n{{/if}} \n"
